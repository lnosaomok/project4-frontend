r =\n        \"grey\";\n    }\n  }, []);\n\n  const submitForm = (e) => {\n    e.preventDefault();\n    setUserPreferences({\n      fatMacro,\n      sugarMacro,\n      carbsMacro,\n      proteinMacro,\n      calories,\n      channels,\n      diet,\n    });\n\n    if (!error) {\n      props.history.push(\"/home\");\n    }\n  };\n\n  return (\n    <div class='container' id='container'>\n      <h4 className='special-font'>Next, tell us your preferences</h4>\n      <form\n        onSubmit={(e) => {\n          submitForm(e);\n        }}\n      >\n        <div id='space'>\n          <span className='special-font-text'>\n            What type of diet do you want to search recipes for? (required)\n          </span>\n        </div>\n        <div id='diet'>\n          {\" \"}\n          <RadioGroup\n            onChange={(e) => {\n              onDietChange(e);\n            }}\n            horizontal\n          >\n            <RadioButton name='diet' value='vegan' checked={diet === \"vegan\"}>\n              Vegan{\" \"}\n            </RadioButton>\n            <RadioButton name='diet' value='keto' checked={diet === \"keto\"}>\n              Ketogenic{\" \"}\n            </RadioButton>\n            <RadioButton name='diet' value='paleo' checked={diet === \"paleo\"}>\n              Paleo{\" \"}\n            </RadioButton>\n            <RadioButton\n              name='diet'\n              value='gluten-free'\n              checked={diet === \"gluten-free\"}\n            >\n              Gluten-free{\" \"}\n            </RadioButton>\n            <RadioButton name='diet' value='all' checked={diet === \"all\"}>\n              All diets{\" \"}\n            </RadioButton>\n          </RadioGroup>\n        </div>\n\n        <div id='space'>\n          <span className='special-font-text'>\n            What channels would you like to recieve suggested recipes from?\n            (optional)\n          </span>\n        </div>\n        <div className='check-group'>\n          <p>\n            <label>\n              <input\n                type='checkbox'\n                value={\"Vegan\"}\n                onChange={(e) => {\n                  onChannelChange(e);\n                }}\n              />\n              <span>Vegan Channel</span>\n            </label>\n          </p>\n          <p>\n            <label>\n              <input\n                type='checkbox'\n                value={\"Keto\"}\n                onChange={(e) => {\n                  onChannelChange(e);\n                }}\n              />\n              <span>Keto Channel</span>\n            </label>\n          </p>\n          <p>\n            <label>\n              <input\n                type='checkbox'\n                value={\"Gluten Free\"}\n                onChange={(e) => {\n                  onChannelChange(e);\n                }}\n              />\n              <span>Gluten-free Channel</span>\n            </label>\n          </p>\n          <p>\n            <label>\n              <input\n                type='checkbox'\n                value={\"Paleo\"}\n                onChange={(e) => {\n                  onChannelChange(e);\n                }}\n              />\n              <span>Paleo Channel</span>\n            </label>\n          </p>\n        </div>\n        <div id='space'>\n          <span className='special-font-text' id='macros'>\n            Tell us any macros you want to monitor (optional)\n          </span>\n        </div>\n        <div>\n          <FormControl\n            className={clsx(classes.margin, classes.textField)}\n            variant='outlined'\n          >\n            <OutlinedInput\n              id='outlined-adornment-weight'\n              value={calories}\n              name='calories'\n              onChange={(e) => {\n                onChangeMacroValues(e);\n              }}\n              endAdornment={\n                <InputAdornment position='end'>kcal</InputAdornment>\n              }\n              aria-describedby='outlined-weight-helper-text'\n              inputProps={{\n                \"aria-label\": \"calories\",\n              }}\n              labelWidth={0}\n            />\n            <FormHelperText id='outlined-weight-helper-text'>\n              Calories (Limit/Target)\n            </FormHelperText>\n          </FormControl>\n        </div>\n        <div>\n          <FormControl\n            className={clsx(classes.margin, classes.textField)}\n            variant='outlined'\n          >\n            <OutlinedInput\n              id='outlined-adornment-weight'\n              value={fatMacro}\n              name='fatMacro'\n              onChange={(e) => {\n                onChangeMacroValues(e);\n              }}\n              endAdornment={\n                <InputAdornment position='end'>grams</InputAdornment>\n              }\n              aria-describedby='outlined-weight-helper-text'\n              inputProps={{\n                \"aria-label\": \"fat\",\n              }}\n              labelWidth={0}\n            />\n            <FormHelperText id='outlined-weight-helper-text'>\n              Fat (Limit/Target)\n            </FormHelperText>\n          </FormControl>\n\n          <FormControl\n            className={clsx(classes.margin, classes.textField)}\n            variant='outlined'\n          >\n            <OutlinedInput\n              id='outlined-adornment-weight'\n              value={carbsMacro}\n              name='carbsMacro'\n              onChange={(e) => {\n                onChangeMacroValues(e);\n              }}\n              endAdornment={\n                <InputAdornment position='end'>grams</InputAdornment>\n              }\n              aria-describedby='outlined-weight-helper-text'\n              inputProps={{\n                \"aria-label\": \"carbs\",\n              }}\n              labelWidth={0}\n            />\n            <FormHelperText id='outlined-weight-helper-text'>\n              Carbs (Limit/Target)\n            </FormHelperText